{
  "name": "pokedatafunc",
  "version": "1.0.0",
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "build": "tsc",
    "copy": "node copy-files.js",
    "prepare": "npm run build && npm run copy",
    "watch": "tsc -w",
    "prestart": "npm run prepare",
    "start": "func start --script-root dist",
    "test": "node test-api.js",
    "test:azure": "node test-azure.js",
    "test:cosmos": "node test-cosmos.js",
    "import": "pnpm exec ts-node import-data.ts",
    "build:deploy": "build-and-deploy.bat",
    "deploy": "func azure functionapp publish pokedata-func --script-root dist",
    "deploy:staging": "func azure functionapp publish pokedata-func --slot staging --script-root dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.15.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@azure/functions": "^3.5.1",
    "@azure/cosmos": "^4.3.0",
    "@azure/storage-blob": "^12.27.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "redis": "^4.7.0"
  }
}
